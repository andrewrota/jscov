    "prepublish": "coffee -co lib src",
    "prestart": "coffee -co lib src"
  },
  "dependencies": {
    "coffee-script": "1.4.0",
    "wrench": "1.4.4",
    "optimist": "0.3.5",
    "underscore": "1.4.3",
    "esprima": "1.0.2",
    "escodegen": "0.0.15"
  },
  "devDependencies": {
    "should": "1.2.1",
    "mocha": "1.7.4"
  }
}
